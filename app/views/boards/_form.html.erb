
    <%= simple_form_for @board, html: {multipart: true} do |f| %>
      
      <%= render 'shared/errors', obj: @board %>

      <div class="form-group">
        <%= f.input :name, input_html: { class: 'form-control' } %>
      </div>
      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>
